body
  color black

svg.stereonet
  fill white

div.plots
  width: 340px*5
  display flex
  flex-flow row wrap

div.sol-plot
  width: 340px
  h2
    margin 0
    margin-left 20px
    margin-bottom -18px
    font-family: "Helvetica Neue", Helvetica, sans-serif
    font-size 1.1em
    color #888

path.error
  stroke-width 1px
  mix-blend-mode soft-light

.labels .label
  fill #000
